<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RegressionTests">
    <listeners>
        <listener class-name="com.example.service.TestListener"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>
    <test name="RegressionTests" parallel="classes" thread-count="1">
        <packages>
            <package name="com.example.test.regression.*"/>
        </packages>
    </test>
</suite>
