<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SmokeTests">
    <listeners>
        <listener class-name="com.example.service.TestListener"/>
    </listeners>
    <test name="SmokeTests" parallel="classes" thread-count="1">
        <packages>
            <package name="com.example.test.smoke.*"/>
        </packages>
    </test>
</suite>
